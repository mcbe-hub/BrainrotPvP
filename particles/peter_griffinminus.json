{
    "format_version": "1.10.0",
    "particle_effect": {
        "description": {
            "identifier": "brainrot:peter_griffinminus",
            "basic_render_parameters": {
                "material": "particles_blend",
                "texture": "textures/particle/peter_griffin.png"
            }
        },
        "components": {
            "minecraft:emitter_rate_steady": {
                "spawn_rate": 100,
                "max_particles": 100
            },
            "minecraft:emitter_lifetime_looping": {
                "active_time": 3
            },
            "minecraft:emitter_shape_point": {
                "offset": [
                    "math.sin(v.emitter_age/v.emitter_lifetime*360)*-75",
                    "30+math.sin(v.emitter_age/v.emitter_lifetime*360)*(0.5-v.emitter_random_1)*90",
                    "math.cos(v.emitter_age/v.emitter_lifetime*360)*75"
                ]
            },
            "minecraft:particle_lifetime_expression": {
                "max_lifetime": "1+v.particle_random_3/4"
            },
            "minecraft:particle_initial_spin": {
                "rotation_rate": 120
            },
            "minecraft:particle_initial_speed": "10+v.particle_random_2*6",
            "minecraft:particle_motion_dynamic": {
                "linear_drag_coefficient": 1.5
            },
            "minecraft:particle_appearance_billboard": {
                "size": [
                    "(2.282*(1-(v.particle_age/v.particle_lifetime)))*(0.5+v.particle_random_3)*2.5",
                    "(3.628*(1-(v.particle_age/v.particle_lifetime)))*(0.5+v.particle_random_3)*2.5"
                ],
                "facing_camera_mode": "rotate_xyz"
            },
            "minecraft:particle_appearance_tinting": {
                "color": {
                    "interpolant": "v.particle_age / v.particle_lifetime",
                    "gradient": {
                        "0.0": "#ffffffff",
                        "0.52": "#ffffffff",
                        "1.0": "#00FFFFFF"
                    }
                }
            }
        }
    }
}