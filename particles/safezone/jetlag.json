{
    "format_version": "1.10.0",
    "particle_effect": {
        "description": {
            "identifier": "rot:jetlag",
            "basic_render_parameters": {
                "material": "particles_blend",
                "texture": "textures/particle/jetlag"
            }
        },
        "components": {
            "minecraft:emitter_rate_steady": {
                "spawn_rate": 4,
                "max_particles": 100
            },
            "minecraft:emitter_lifetime_looping": {
                "active_time": 1
            },
            "minecraft:emitter_shape_point": {},
            "minecraft:particle_lifetime_expression": {
                "max_lifetime": "3+v.particle_random_1*2"
            },
            "minecraft:particle_motion_parametric": {
                "relative_position": [
                    "((v.particle_random_1 > 0.5 ? 1 : -1))*math.sin((v.particle_age/v.particle_lifetime)*360)*(120-(v.particle_random_2*45))",
                    "v.particle_random_2*100",
                    "math.cos((v.particle_age/v.particle_lifetime)*360)*(120-(v.particle_random_2*45))"
                ]
            },
            "minecraft:particle_appearance_billboard": {
                "size": [
                    "(3+v.particle_random_3*6)*2",
                    "(3+v.particle_random_3*6)"
                ],
                "facing_camera_mode": "rotate_xyz"
            },
            "minecraft:particle_appearance_tinting": {
                "color": {
                    "interpolant": "v.particle_age / v.particle_lifetime",
                    "gradient": {
                        "0.0": "#00FFFFFF",
                        "0.37": "#ffffffff",
                        "0.6": "#ffffffff",
                        "1.0": "#00FFFFFF"
                    }
                }
            }
        }
    }
}